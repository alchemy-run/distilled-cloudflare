{
  "name": "distilled-cloudflare",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "download:env": "doppler secrets download --no-file --format env > .env",
    "find": "bun scripts/find-errors/index.ts",
    "generate": "bun scripts/generate-clients.ts && bun format",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "tsc -b",
    "format": "oxfmt './src' './test' './scripts'"
  },
  "dependencies": {
    "effect": "latest",
    "@effect/platform": "latest"
  },
  "devDependencies": {
    "@effect/cli": "latest",
    "@effect/platform-node": "latest",
    "@effect/vitest": "latest",
    "@types/bun": "latest",
    "oxfmt": "^0.24.0",
    "vitest": "latest"
  },
  "exports": {
    "./*": {
      "bun": "./src/services/*.ts"
    },
    "./Auth": {
      "bun": "./src/auth.ts"
    },
    "./Category": {
      "bun": "./src/category.ts"
    },
    "./Errors": {
      "bun": "./src/errors.ts"
    },
    "./Traits": {
      "bun": "./src/traits.ts"
    },
    ".": {
      "bun": "./src/index.ts"
    }
  }
}
